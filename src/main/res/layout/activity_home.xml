<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <TextView
        android:id="@+id/textView57"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/roundedge2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView70"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView70"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/doumb"
        android:gravity="center"
        android:text="메뉴"
        android:textColor="#0363A3"
        android:textSize="@dimen/_32ssp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView57">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="36"
            android:gravity="center_horizontal|top"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView86"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="#00FFFFFF"
                android:textSize="@dimen/_37ssp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_weight="27"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView67"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="#34FFFFFF"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/button3"
                    android:layout_width="@dimen/_130sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginBottom="@dimen/_2sdp"
                    android:background="#52000000"
                    android:fontFamily="@font/doumb"
                    android:onClick="onClick2"
                    android:text="통계보기"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/_25ssp"
                    app:layout_constraintBottom_toBottomOf="@+id/textView67"
                    app:layout_constraintEnd_toEndOf="@+id/textView67"
                    app:layout_constraintHorizontal_bias="0.85"
                    app:layout_constraintStart_toStartOf="@+id/textView67"
                    app:layout_constraintTop_toTopOf="@+id/textView67"
                    app:layout_constraintVertical_bias="0.876" />

                <ImageView
                    android:id="@+id/imageView52"
                    android:layout_width="@dimen/_110sdp"
                    android:layout_height="@dimen/_130sdp"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toBottomOf="@+id/textView67"
                    app:layout_constraintEnd_toEndOf="@+id/textView67"
                    app:layout_constraintHorizontal_bias="0.058"
                    app:layout_constraintStart_toStartOf="@+id/textView67"
                    app:layout_constraintTop_toTopOf="@+id/textView67"
                    app:layout_constraintVertical_bias="0.538"
                    app:srcCompat="@drawable/exam" />

                <TextView
                    android:id="@+id/textView66"
                    android:layout_width="@dimen/_170sdp"
                    android:layout_height="@dimen/_80sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:fontFamily="@font/doumb"
                    android:gravity="center"
                    android:text=" 지난 점수를 그래프로 \n확인해 보세요."
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/_18ssp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView67"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="@+id/textView67"
                    app:layout_constraintTop_toTopOf="@+id/textView67"
                    app:layout_constraintVertical_bias="0.0" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="39"
            android:gravity="center_horizontal|top"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView88"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="#00FFFFFF"
                android:textSize="@dimen/_37ssp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_weight="27"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView68"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="#34FFFFFF"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/imageView50"
                    android:layout_width="@dimen/_110sdp"
                    android:layout_height="@dimen/_150sdp"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toBottomOf="@+id/textView68"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.066"
                    app:layout_constraintStart_toStartOf="@+id/textView68"
                    app:layout_constraintTop_toTopOf="@+id/textView68"
                    app:layout_constraintVertical_bias="0.592"
                    app:srcCompat="@drawable/graph" />

                <Button
                    android:id="@+id/button4"
                    android:layout_width="@dimen/_130sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginBottom="@dimen/_2sdp"
                    android:background="#52000000"
                    android:fontFamily="@font/doumb"
                    android:onClick="onClick"
                    android:text="문제풀기"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/_25ssp"
                    app:layout_constraintBottom_toBottomOf="@+id/textView68"
                    app:layout_constraintEnd_toEndOf="@+id/textView68"
                    app:layout_constraintHorizontal_bias="0.85"
                    app:layout_constraintStart_toStartOf="@+id/textView68"
                    app:layout_constraintTop_toTopOf="@+id/textView68"
                    app:layout_constraintVertical_bias="0.876" />

                <TextView
                    android:id="@+id/textView69"
                    android:layout_width="@dimen/_170sdp"
                    android:layout_height="@dimen/_80sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:fontFamily="@font/doumb"
                    android:gravity="center"
                    android:text="인지활동 프로그램을\n시작합니다."
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/_18ssp"
                    app:layout_constraintBottom_toBottomOf="@+id/textView68"
                    app:layout_constraintEnd_toEndOf="@+id/textView68"
                    app:layout_constraintHorizontal_bias="0.988"
                    app:layout_constraintStart_toStartOf="@+id/textView68"
                    app:layout_constraintTop_toTopOf="@+id/textView68"
                    app:layout_constraintVertical_bias="0.0" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="34"
            android:gravity="center|center_horizontal"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button80"
                android:layout_width="@dimen/_170sdp"
                android:layout_height="@dimen/_50sdp"
                android:background="#39000000"
                android:fontFamily="@font/doumb"
                android:onClick="onClick3"
                android:text="사용자 전환"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/_25ssp" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>